<project>
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.jason</groupId>
	<artifactId>parent-pom</artifactId>
	<version>1.0</version>
	<packaging>pom</packaging>

	<name>Jason Parent POM</name>
	<description>Some defaults for Maven builds</description>
	<url>it@github.com:JasonZ-Git/JasonTechnology.git</url>
	<licenses>
		<license>
			<name>The Apache License, Version 2.0</name>
		</license>
	</licenses>

	<developers>
		<developer>
			<id>jason.zhang</id>
			<name>Jason Zhang</name>
			<email>zclok236@gmail.com</email>
			<url>http://www.jasonzhang.com.au</url>
			<organization>techgarden</organization>
			<organizationUrl>http://www.jasonzhang.com.au</organizationUrl>
			<roles>
				<role>Architecture</role>
